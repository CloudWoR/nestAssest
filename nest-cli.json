{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/server/src",
  "monorepo": true,
  "root": "apps/server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/server/tsconfig.app.json"
  },
  "projects": {
    "server": {
      "type": "application",
      "root": "apps/server",
      "entryFile": "main",
      "sourceRoot": "apps/server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server/tsconfig.app.json",
        "assets": [
          {
            "include": "../config/*.yaml",
            "outDir": "./dist/config",
            "watchAssets": true
          },
          {
            "include": "../docker/*.*",
            "outDir": "./dist/docker",
            "watchAssets": true
          }
        ]
      }
    },
    "file-service": {
      "type": "application",
      "root": "apps/file-service",
      "entryFile": "main",
      "sourceRoot": "apps/file-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file-service/tsconfig.app.json"
      }
    },
    "film-service": {
      "type": "application",
      "root": "apps/film-service",
      "entryFile": "main",
      "sourceRoot": "apps/film-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/film-service/tsconfig.app.json"
      }
    },
    "report-service": {
      "type": "application",
      "root": "apps/report-service",
      "entryFile": "main",
      "sourceRoot": "apps/report-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/report-service/tsconfig.app.json"
      }
    },
    "printer-service": {
      "type": "application",
      "root": "apps/printer-service",
      "entryFile": "main",
      "sourceRoot": "apps/printer-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/printer-service/tsconfig.app.json"
      }
    },
    "system-logger": {
      "type": "library",
      "root": "libs/system-logger",
      "entryFile": "index",
      "sourceRoot": "libs/system-logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/system-logger/tsconfig.lib.json"
      }
    },
    "broker-nats": {
      "type": "application",
      "root": "apps/broker-nats",
      "entryFile": "main",
      "sourceRoot": "apps/broker-nats/src",
      "compilerOptions": {
        "tsConfigPath": "apps/broker-nats/tsconfig.app.json"
      }
    },
    "broker-redis": {
      "type": "application",
      "root": "apps/broker-redis",
      "entryFile": "main",
      "sourceRoot": "apps/broker-redis/src",
      "compilerOptions": {
        "tsConfigPath": "apps/broker-redis/tsconfig.app.json"
      }
    }
  }
}